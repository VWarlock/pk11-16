INILAB:
	CLR	-2(R5)		; Эту подпрограмму больше не вызывать .
	MOV	NUMLAB,R0	; Номер лабиринта .
	MOV	R0,R1		; Ё
	ASR	R1		; Записать номер лабиринта для вывода .
	INC	R1		; Ѕ
	MOV	R1,NUM.LAB	; Ђ
	MOV	#2,SPEED	; Нормальная скорость движения .
