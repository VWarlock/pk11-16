TIMCALL:
	MOV	#BUFMLT,R5	; Буфер многозадачности .
1$:	TST	(R5)		; Вызывать надо ?
	BEQ	2$		; Нет .
	DEC	(R5)+		; Пора пришла ?
	BNE	3$		; Нет .
	TST	(R5)		; Есть подпрограмма ?
	BEQ	3$		; Нет .
	CALL	@(R5)+		; Вызвать подпрограмму .
	TST	(R5)+		; Пропустить параметр .
	BR	4$		; Продолжить .
2$:	TST	(R5)+		; +2 .
3$:	CMP	(R5)+,(R5)+	; +4 .
4$:	CMP	R5,#BUFMLT+<NUMPRC*6>; Дошли до конца буфера ?
	BLO	1$		; Нет .
	RETURN
;ЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄЄ
;				ИНИЦИАЦИЯ ИГРЫ .
