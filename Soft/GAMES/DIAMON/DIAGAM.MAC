INIGAME:
	MOV	#BUFMLT,R0	; Ё
	MOV	#NUMPRC*3,R1	; Очистить буфер многозадачности .
1$:	CLR	(R0)+		; Ѕ
	SOB	R1,1$		; Ђ
	MOV	#2$,R0		; Ё
	CALL	PUTPRC		; Запустить сканирование клавиатуры .
	CALL	PUTPRC		; Запустить вывод лабиринта .
	CLR	NUMLAB		; 0 лабиринт .
