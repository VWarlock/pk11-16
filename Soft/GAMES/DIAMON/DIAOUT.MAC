	BIC	(R4)+,R1	; Ѕ
	ASH	(R4)+,R2	; Расчитать цвет .
	BIC	(R4)+,R2	; Ѕ
	BIS	R2,R1		; Ђ
	MOV	R1,(R3)+	; Занести цвет .
	SOB	R5,1$		; Повторить для всех .
	RETURN
